#include "holberton.h"
/**
 * _strlen - Entry point
 * @s: Is the parameter to be checked
 * Return: a
 */
int _strlen(char *s)
{
	int a;

	for (a = 0; s[a] != 0; a++)
		;
	return (a);
}
/**
 *  *_strdup - Entry point
 * @str: Is the parameter to be checked
 * Return: s
 */
char *_strdup(char *str)
{
	char *s;
	int q, w;

	if (str == 0)
		return (NULL);
	for (w = 0; str[w] != '\0'; w++)
		;
	s = malloc(1 + sizeof(char) * w);
	if (s == NULL)
		return (NULL);
	for (q = 0; q < w; q++)
		s[q] = str[q];
	return (s);
}
